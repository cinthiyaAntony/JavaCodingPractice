class Main {
    public static void main(String[] args) {
          int n =5 ;
        for (int i=0; i<n; i++){
            System.out.println(fab(i)+ " ");
        }

            }
        
 public static int fab(int n){
      
      if(n<=1){
          return n;
      }
      return fab(n-1) + fab(n-2);
     
 }        
}

output:
0 1  1 2 3 
